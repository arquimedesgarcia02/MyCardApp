{
  "expo": {
    "name": "MyCardApp",
    "slug": "MyCardApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/app_images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f5f9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_images/adaptive-icon.png",
        "backgroundColor": "#f1f5f9"
      },
      "package": "com.ag.mycardapp",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow MyCardApp to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "07395c79-c137-4895-a972-fbd3bce7695b"
      }
    }
  }
}
